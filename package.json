{
  "name": "@jsenv/core",
  "version": "5.131.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jsenv/jsenv-core"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/commonjs/main.js",
  "module": "index.js",
  "files": [
    "/dist/",
    "/src/",
    "/index.js"
  ],
  "scripts": {
    "test": "node ./script/test/test.js",
    "eslint-check": "eslint .",
    "prettier-check": "node ./script/prettier-check/prettier-check.js",
    "generate-coverage": "node ./script/generate-coverage/generate-coverage.js",
    "upload-coverage": "node ./script/upload-coverage/upload-coverage.js",
    "generate-commonjs-bundle": "node ./script/generate-commonjs-bundle/generate-commonjs-bundle.js",
    "generate-import-map": "node ./script/generate-import-map/generate-import-map.js",
    "dist": "npm run clean && npm run generate-commonjs-bundle",
    "clean": "rimraf dist && rimraf coverage",
    "reinstall": "npm run clean && rimraf node_modules && npm install",
    "postinstall": "npm run generate-import-map",
    "prepublishOnly": "node ./script/transform-package/remove-postinstall.js && npm run dist",
    "postpublish": "node ./script/transform-package/restore-postinstall.js"
  },
  "dependencies": {
    "@jsenv/bundling": "1.13.0",
    "@jsenv/grouping": "1.1.0",
    "@jsenv/module-resolution": "3.3.0",
    "@jsenv/babel-plugin-map": "1.0.0",
    "@jsenv/operating-system-path": "2.1.0",
    "@dmail/cancellation": "2.5.0",
    "@dmail/helper": "2.2.0",
    "@dmail/project-structure": "3.3.0",
    "@dmail/filesystem-matching": "1.3.0",
    "@dmail/filesystem-watch": "2.6.0",
    "@dmail/uneval": "5.4.0",
    "@dmail/server": "2.3.0",
    "@dmail/process-signals": "1.0.0",
    "@babel/core": "7.2.2",
    "@babel/helper-hoist-variables": "7.0.0",
    "@babel/helper-plugin-utils": "7.0.0",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-syntax-import-meta": "7.2.0",
    "babel-plugin-transform-commonjs": "1.1.5",
    "abort-controller": "2.0.2",
    "ansi-to-html": "0.6.9",
    "eventsource": "1.0.7",
    "magic-string": "0.25.2",
    "node-fetch": "2.3.0",
    "proper-lockfile": "3.2.0",
    "istanbul-lib-coverage": "2.0.1",
    "systemjs": "3.1.1",
    "rollup": "1.16.4",
    "rollup-plugin-commonjs": "10.0.1",
    "rollup-plugin-json": "4.0.0",
    "rollup-plugin-node-builtins": "2.1.2",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-node-globals": "1.4.0",
    "rollup-plugin-replace": "2.2.0"
  },
  "devDependencies": {
    "@jsenv/core": "5.129.0",
    "@jsenv/testing": "1.23.0",
    "@jsenv/chromium-launcher": "2.1.0",
    "@jsenv/node-launcher": "1.14.0",
    "@jsenv/node-module-import-map": "3.2.0",
    "@jsenv/eslint-config": "9.7.0",
    "@jsenv/prettier-config": "1.0.0",
    "@jsenv/prettier-check-project": "3.2.0",
    "@jsenv/codecov-upload": "1.6.0",
    "@dmail/assert": "3.13.0",
    "eslint": "5.1.0",
    "prettier": "1.18.2",
    "puppeteer": "1.12.2",
    "react": "16.8.6",
    "rimraf": "2.6.2"
  }
}
