{
  "name": "@jsenv/core",
  "version": "20.1.3",
  "description": "Tool to develop, test and build js projects",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jsenv/jsenv-core"
  },
  "engines": {
    "node": ">=14.9.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./main.js"
    },
    "./*": "./*"
  },
  "main": "main.js",
  "files": [
    "/dist/",
    "/helpers/",
    "/src/",
    "/main.js"
  ],
  "scripts": {
    "eslint-check": "node ./node_modules/eslint/bin/eslint.js . --ext=.js,.mjs,.html",
    "generate-importmap": "node ./script/importmap/generate_importmap.js",
    "build": "node ./script/build/build.js",
    "dist": "npm run build",
    "test": "node --experimental-import-meta-resolve ./script/test/test.js",
    "start-exploring": "node ./script/start-exploring/start-exploring.js",
    "test-with-coverage": "npm run test -- --coverage",
    "test-js-transform": "node ./script/test/test-js-transform.js",
    "test-compile-server": "node ./script/test/test-compile-server.js",
    "test-execute-test-plan": "node ./script/test/test-execute-test-plan.js",
    "test-build": "node ./script/test/test_build.js",
    "test-launch-browser": "node ./script/test/test-launch-browser.js",
    "test-launch-node": "node ./script/test/test-launch-node.js",
    "measure-performances": "node ./script/performance/generate_performance_report.js --local",
    "measure-test-plan-performances": "node ./script/performance/measure_test_plan/measure_test_plan.js --local",
    "measure-import-performances": "node ./script/performance/measure_import/measure_import.js --local",
    "prettier-format": "node ./script/prettier/prettier_format.mjs",
    "prettier-format-stage": "npm run prettier-format -- --staged",
    "prettier-check": "npm run prettier-format -- --dry-run",
    "install-playwright": "npx playwright install-deps && npx playwright install",
    "link-packages": "node ./script/link-packages/link-packages.js",
    "postinstall": "npm run generate-importmap && npm run install-playwright",
    "prepublishOnly": "node ./script/publish/remove_postinstall.mjs && npm run dist",
    "postpublish": "node ./script/publish/restore_postinstall.mjs"
  },
  "dependencies": {
    "@babel/core": "7.15.5",
    "@babel/helper-module-imports": "7.15.4",
    "@babel/helpers": "7.15.4",
    "@babel/parser": "7.15.6",
    "@babel/plugin-proposal-dynamic-import": "7.14.5",
    "@babel/plugin-proposal-json-strings": "7.14.5",
    "@babel/plugin-proposal-numeric-separator": "7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "7.15.6",
    "@babel/plugin-proposal-optional-catch-binding": "7.14.5",
    "@babel/plugin-proposal-optional-chaining": "7.14.5",
    "@babel/plugin-proposal-unicode-property-regex": "7.14.5",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-syntax-import-meta": "7.10.4",
    "@babel/plugin-syntax-numeric-separator": "7.10.4",
    "@babel/plugin-syntax-object-rest-spread": "7.8.3",
    "@babel/plugin-syntax-optional-catch-binding": "7.8.3",
    "@babel/plugin-transform-arrow-functions": "7.14.5",
    "@babel/plugin-transform-block-scoped-functions": "7.14.5",
    "@babel/plugin-transform-block-scoping": "7.15.3",
    "@babel/plugin-transform-classes": "7.15.4",
    "@babel/plugin-transform-computed-properties": "7.14.5",
    "@babel/plugin-transform-destructuring": "7.14.7",
    "@babel/plugin-transform-dotall-regex": "7.14.5",
    "@babel/plugin-transform-duplicate-keys": "7.14.5",
    "@babel/plugin-transform-exponentiation-operator": "7.14.5",
    "@babel/plugin-transform-for-of": "7.15.4",
    "@babel/plugin-transform-function-name": "7.14.5",
    "@babel/plugin-transform-literals": "7.14.5",
    "@babel/plugin-transform-modules-systemjs": "7.15.4",
    "@babel/plugin-transform-new-target": "7.14.5",
    "@babel/plugin-transform-object-super": "7.14.5",
    "@babel/plugin-transform-parameters": "7.15.4",
    "@babel/plugin-transform-regenerator": "7.14.5",
    "@babel/plugin-transform-shorthand-properties": "7.14.5",
    "@babel/plugin-transform-spread": "7.14.6",
    "@babel/plugin-transform-sticky-regex": "7.14.5",
    "@babel/plugin-transform-template-literals": "7.14.5",
    "@babel/plugin-transform-typeof-symbol": "7.14.5",
    "@babel/plugin-transform-unicode-regex": "7.14.5",
    "@c88/v8-coverage": "0.1.1",
    "@jsenv/cancellation": "3.0.0",
    "@jsenv/filesystem": "2.1.1",
    "@jsenv/import-map": "6.13.3",
    "@jsenv/logger": "4.0.1",
    "@jsenv/node-signals": "2.0.1",
    "@jsenv/server": "7.1.0",
    "@jsenv/uneval": "1.6.0",
    "@rollup/plugin-commonjs": "20.0.0",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "13.0.4",
    "@rollup/plugin-replace": "3.0.0",
    "ansi-to-html": "0.7.1",
    "babel-plugin-transform-async-to-promises": "0.8.15",
    "bytes": "3.1.0",
    "cssnano": "5.0.8",
    "cssnano-preset-default": "5.1.4",
    "cuid": "2.1.8",
    "estree-walker": "3.0.0",
    "html-minifier": "4.0.0",
    "humanize-duration": "3.27.0",
    "is-unicode-supported": "1.1.0",
    "istanbul-lib-coverage": "3.0.0",
    "istanbul-lib-instrument": "4.0.3",
    "istanbul-lib-report": "3.0.0",
    "istanbul-reports": "3.0.2",
    "magic-string": "0.25.7",
    "parse5": "6.0.1",
    "playwright": "1.14.1",
    "postcss": "8.3.6",
    "postcss-value-parser": "4.1.0",
    "regenerator-runtime": "0.13.9",
    "rollup": "2.56.3",
    "rollup-plugin-node-builtins-brofs": "2.1.3",
    "rollup-plugin-node-globals": "1.4.0",
    "source-map": "0.7.3",
    "string-width": "5.0.0",
    "supports-color": "9.0.2",
    "systemjs": "6.10.3",
    "terser": "5.7.2",
    "tree-kill": "1.2.2",
    "v8-to-istanbul": "8.0.0",
    "wrap-ansi": "8.0.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.15.4",
    "@babel/plugin-transform-react-jsx": "7.14.9",
    "@babel/plugin-transform-typescript": "7.15.4",
    "@jsenv/assert": "2.3.1",
    "@jsenv/codecov-upload": "3.5.0",
    "@jsenv/eslint-config": "16.0.1",
    "@jsenv/github-release-package": "1.2.3",
    "@jsenv/importmap-eslint-resolver": "5.1.2",
    "@jsenv/importmap-node-module": "2.2.0",
    "@jsenv/node-module-import-map": "13.6.1",
    "@jsenv/package-publish": "1.6.2",
    "@jsenv/performance-impact": "1.7.0",
    "@jsenv/prettier-check-project": "5.6.1",
    "@jsenv/pwa": "4.0.0",
    "@vue/component-compiler": "4.2.4",
    "eslint": "7.32.0",
    "eslint-plugin-html": "6.1.2",
    "eslint-plugin-import": "2.24.2",
    "node-notifier": "10.0.0",
    "prettier": "2.4.0",
    "react": "17.0.2",
    "redux": "4.1.1",
    "sass": "1.39.2",
    "vue-template-compiler": "2.6.14"
  }
}
