{
  "name": "@jsenv/core",
  "version": "27.0.0",
  "description": "Tool to develop, test and build js projects",
  "license": "MIT",
  "author": {
    "name": "dmail",
    "email": "dmaillard06@gmail.com",
    "url": "https://twitter.com/damienmaillard"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jsenv/jsenv-core"
  },
  "engines": {
    "node": ">=16.13.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "type": "module",
  "imports": {},
  "exports": {
    ".": {
      "import": {
        "development": "./src/main.js",
        "default": "./dist/main.js"
      }
    },
    "./*": "./*"
  },
  "main": "./src/main.js",
  "files": [
    "/dist/",
    "/src/"
  ],
  "workspaces": [
    "./packages/*",
    "./packages/create-jsenv/*"
  ],
  "scripts": {
    "eslint": "npx eslint . --ext=.js,.mjs,.cjs,.html",
    "dev": "node --conditions=development ./scripts/dev/dev.mjs",
    "test": "node --conditions=development ./scripts/test/test.mjs",
    "test-packages": "npm run test --workspaces --if-present -- --workspace",
    "build": "node --conditions=development ./scripts/build/build.mjs",
    "start_file_server": "node ./scripts/dev/start_file_server.mjs",
    "workspace-versions": "node ./scripts/publish/workspace_versions.mjs",
    "workspace-publish": "node ./scripts/publish/workspace_publish.mjs",
    "performances": "node --expose-gc ./scripts/performance/generate_performance_report.mjs --log --once",
    "file-size": "node ./scripts/file_size/file_size.mjs --log",
    "prettier": "prettier --write .",
    "playwright-install": "npx playwright install-deps && npx playwright install",
    "certificate-install": "node ./scripts/dev/install_certificate_authority.mjs",
    "test-with-coverage": "npm run test -- --coverage",
    "prepublishOnly": "npm run build"
  },
  "optionalDependencies": {
    "playwright": "1.x"
  },
  "dependencies": {
    "@babel/plugin-proposal-dynamic-import": "7.16.7",
    "@babel/plugin-transform-modules-systemjs": "7.18.5",
    "@babel/plugin-transform-modules-umd": "7.18.0",
    "@c88/v8-coverage": "0.1.1",
    "@financial-times/polyfill-useragent-normaliser": "2.0.1",
    "@jsenv/ast": "1.1.1",
    "@jsenv/abort": "4.2.3",
    "@jsenv/filesystem": "4.1.0",
    "@jsenv/importmap": "1.2.1",
    "@jsenv/integrity": "0.0.1",
    "@jsenv/node-esm-resolution": "0.1.0",
    "@jsenv/server": "12.7.4",
    "@jsenv/uneval": "1.6.0",
    "@jsenv/url-meta": "7.0.0",
    "@jsenv/urls": "1.2.6",
    "@jsenv/utils": "2.0.1",
    "@jsenv/babel-plugins": "1.0.5",
    "@jsenv/log": "2.1.1",
    "@jsenv/sourcemap": "1.0.1",
    "acorn-import-assertions": "1.8.0",
    "cuid": "2.1.8",
    "html-minifier": "4.0.0",
    "istanbul-lib-coverage": "3.2.0",
    "istanbul-lib-instrument": "5.2.0",
    "istanbul-lib-report": "3.0.0",
    "istanbul-reports": "3.1.4",
    "pidtree": "0.6.0",
    "rollup": "2.75.7",
    "string-width": "5.1.2",
    "strip-ansi": "7.0.1",
    "terser": "5.14.1",
    "v8-to-istanbul": "9.0.1",
    "wrap-ansi": "8.0.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.18.2",
    "@babel/plugin-syntax-import-assertions": "7.17.12",
    "@jsenv/assert": "2.6.0",
    "@jsenv/eslint-config": "16.0.9",
    "@jsenv/file-size-impact": "13.0.0",
    "@jsenv/https-local": "2.1.0",
    "@jsenv/package-workspace": "0.3.0",
    "@jsenv/performance-impact": "3.0.0",
    "eslint": "8.18.0",
    "eslint-plugin-html": "6.2.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-react": "7.30.1",
    "playwright": "1.22.2",
    "prettier": "2.7.1"
  }
}
