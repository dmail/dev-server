{
  "name": "@jsenv/core",
  "version": "34.2.1",
  "description": "Tool to develop, test and build js projects",
  "license": "MIT",
  "author": {
    "name": "dmail",
    "email": "dmaillard06@gmail.com",
    "url": "https://twitter.com/damienmaillard"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jsenv/jsenv-core"
  },
  "engines": {
    "node": ">=18.5.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "type": "module",
  "imports": {},
  "exports": {
    ".": {
      "import": {
        "development": "./src/main.js",
        "default": "./dist/jsenv.js"
      }
    },
    "./*": "./*"
  },
  "main": "./src/main.js",
  "files": [
    "/dist/",
    "/src/"
  ],
  "workspaces": [
    "./packages/*",
    "./packages/create-jsenv/*"
  ],
  "scripts": {
    "eslint": "npx eslint . --ext=.js,.mjs,.cjs,.html",
    "dev": "node --conditions=development ./scripts/dev/dev.mjs",
    "test": "node --conditions=development ./scripts/test/test.mjs",
    "test:dev_errors_snapshots": "node --conditions=development ./tests/dev_server/errors/generate_snapshot_files.mjs",
    "test:workspace": "npm run test --workspaces --if-present -- --workspace",
    "build": "node --conditions=development ./scripts/build/build.mjs",
    "build:file_size": "node ./scripts/build/build_file_size.mjs --log",
    "workspace:versions": "node ./scripts/publish/workspace_versions.mjs",
    "workspace:publish": "node ./scripts/publish/workspace_publish.mjs",
    "performances": "node --expose-gc ./scripts/performance/generate_performance_report.mjs --log --once",
    "prettier": "prettier --write .",
    "playwright:install": "npx playwright install-deps && npx playwright install",
    "certificate:install": "node ./scripts/dev/install_certificate_authority.mjs",
    "prepublishOnly": "npm run build"
  },
  "optionalDependencies": {
    "playwright": "1.x"
  },
  "dependencies": {
    "@babel/plugin-proposal-dynamic-import": "7.18.6",
    "@babel/plugin-transform-modules-systemjs": "7.20.11",
    "@babel/plugin-transform-modules-umd": "7.18.6",
    "@c88/v8-coverage": "0.1.1",
    "@financial-times/polyfill-useragent-normaliser": "1.10.2",
    "@jsenv/abort": "4.2.4",
    "@jsenv/ast": "3.0.5",
    "@jsenv/babel-plugins": "1.1.5",
    "@jsenv/filesystem": "4.2.3",
    "@jsenv/importmap": "1.2.1",
    "@jsenv/integrity": "0.0.1",
    "@jsenv/log": "3.3.4",
    "@jsenv/node-esm-resolution": "1.0.1",
    "@jsenv/server": "15.0.2",
    "@jsenv/sourcemap": "1.0.10",
    "@jsenv/uneval": "1.6.0",
    "@jsenv/url-meta": "8.1.0",
    "@jsenv/urls": "2.0.0",
    "@jsenv/utils": "2.0.1",
    "@paralleldrive/cuid2": "2.2.0",
    "construct-style-sheets-polyfill": "3.1.0",
    "istanbul-lib-coverage": "3.2.0",
    "istanbul-lib-instrument": "5.2.1",
    "istanbul-lib-report": "3.0.0",
    "istanbul-reports": "3.1.5",
    "launch-editor": "2.6.0",
    "lightningcss": "1.19.0",
    "pidtree": "0.6.0",
    "string-width": "5.1.2",
    "strip-ansi": "7.0.1",
    "v8-to-istanbul": "9.1.0",
    "webidl2": "24.2.2",
    "wrap-ansi": "8.1.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.21.3",
    "@babel/plugin-syntax-import-assertions": "7.20.0",
    "@jsenv/assert": "./packages/assert/",
    "@jsenv/eslint-config": "./packages/eslint-config/",
    "@jsenv/file-size-impact": "14.1.0",
    "@jsenv/https-local": "3.0.6",
    "@jsenv/package-workspace": "0.5.1",
    "@jsenv/performance-impact": "4.1.0",
    "@jsenv/plugin-globals": "./packages/jsenv-plugin-globals/",
    "@jsenv/plugin-minification": "./packages/jsenv-plugin-minification/",
    "@jsenv/plugin-placeholders": "./packages/jsenv-plugin-placeholders/",
    "@jsenv/plugin-as-js-classic": "./packages/jsenv-plugin-as-js-classic/",
    "eslint": "8.38.0",
    "eslint-plugin-html": "7.1.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-react": "7.32.2",
    "playwright": "1.32.3",
    "prettier": "2.8.7"
  }
}
