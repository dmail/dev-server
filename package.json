{
	"name": "@dmail/dev-server",
	"version": "1.0.0",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/dmail/dev-server"
	},
	"publishConfig": {
		"access": "public"
	},
	"main": "dist/index.js",
	"module": "index.js",
	"files": [
		"dist",
		"src",
		"index.js"
	],
	"engines": {
		"node": ">=8.6.0"
	},
	"dependencies": {
		"@babel/core": "7.1.0",
		"@dmail/cancellation": "github:dmail/cancellation",
		"@dmail/module-loader": "github:dmail/module-loader",
		"@dmail/project-structure": "github:dmail/project-structure",
		"@dmail/project-structure-compile-babel": "github:dmail/project-structure-compile-babel",
		"@dmail/signal": "9.2.0",
		"@dmail/uneval": "4.0.0",
		"eventsource": "1.0.7",
		"istanbul": "0.4.5",
		"istanbul-lib-coverage": "2.0.1",
		"istanbul-lib-instrument": "3.0.0",
		"kill-port": "1.3.2",
		"node-fetch": "2.0.0",
		"systemjs": "2.0.2",
		"node-forge": "0.7.1",
		"puppeteer": "1.10.0",
		"rollup": "0.66.2",
		"rollup-plugin-babel": "4.0.3",
		"rollup-plugin-node-resolve": "3.4.0",
		"rollup-plugin-replace": "2.0.0"
	},
	"devDependencies": {
		"@dmail/assert": "1.0.0",
		"codecov": "2.3.0",
		"mock-fs": "4.4.2",
		"@dmail/project-eslint-config": "github:dmail/project-eslint-config",
		"eslint": "5.1.0",
		"eslint-plugin-import": "2.6.0",
		"babel-eslint": "9.0.0",
		"@dmail/project-prettier-config": "github:dmail/project-prettier-config",
		"@dmail/prettiest": "github:dmail/prettiest",
		"prettier": "1.9.1",
		"rimraf": "2.6.2"
	},
	"scripts": {
		"lint": "eslint .",
		"format": "prettiest",
		"compile": "node ./script/compile.js",
		"compile-list": "node ./script/compile-list.js",
		"clean": "rimraf dist && rimraf coverage && rimraf .nyc_output",
		"dist": "npm run clean && npm run compile",
		"test": "npm run dist && node ./dist/index.test.js",
		"cover": "npm run dist && nyc node ./dist/index.test.js",
		"coverage-upload": "codecov",
		"prepublishOnly": "npm run dist",
		"reinstall": "npm run clean && rimraf node_modules && npm install"
	}
}
