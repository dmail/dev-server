{
  "name": "@jsenv/core",
  "version": "27.0.0-alpha.28",
  "description": "Tool to develop, test and build js projects",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jsenv/jsenv-core"
  },
  "engines": {
    "node": ">=16.13.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "imports": {},
  "exports": {
    ".": {
      "import": "./main.js"
    },
    "./*": "./*"
  },
  "main": "main.js",
  "files": [
    "/dist/",
    "/src/",
    "/main.js"
  ],
  "workspaces": [
    "./packages/*"
  ],
  "scripts": {
    "eslint": "npx eslint ./test/dev_server/manual/main/importmap_inline/ --ext=.js,.mjs,.cjs,.html",
    "build": "node ./script/build/build.mjs",
    "test": "node ./script/test/test.mjs",
    "test-with-coverage": "npm run test -- --coverage",
    "dev": "node ./script/dev/dev.mjs",
    "start_file_server": "node ./script/dev/start_file_server.mjs",
    "performances": "node --expose-gc ./script/performance/generate_performance_report.mjs --log --once",
    "file-size": "node ./script/file_size/file_size.mjs --log",
    "prettier": "prettier --write .",
    "playwright-install": "npx playwright install-deps && npx playwright install",
    "certificate-install": "node ./script/dev/install_certificate_authority.mjs",
    "workspace-versions": "node ./script/publish/workspace_versions.mjs",
    "workspace-publish": "node ./script/publish/workspace_publish.mjs",
    "prepublishOnly": "npm run build"
  },
  "optionalDependencies": {
    "playwright": "1.x"
  },
  "dependencies": {
    "@babel/parser": "7.17.9",
    "@babel/plugin-proposal-dynamic-import": "7.16.7",
    "@babel/plugin-transform-modules-systemjs": "7.17.8",
    "@babel/plugin-transform-modules-umd": "7.16.7",
    "@c88/v8-coverage": "0.1.1",
    "@financial-times/polyfill-useragent-normaliser": "2.0.1",
    "@jsenv/abort": "4.1.2",
    "@jsenv/babel-plugins": "1.0.2",
    "@jsenv/filesystem": "3.1.0",
    "@jsenv/importmap": "1.2.0",
    "@jsenv/integrity": "0.0.1",
    "@jsenv/log": "1.5.1",
    "@jsenv/logger": "4.0.1",
    "@jsenv/node-esm-resolution": "0.0.4",
    "@jsenv/server": "12.6.1",
    "@jsenv/uneval": "1.6.0",
    "@jsenv/utils": "1.4.2",
    "construct-style-sheets-polyfill": "3.1.0",
    "cssnano": "5.1.7",
    "cssnano-preset-default": "5.2.7",
    "cuid": "2.1.8",
    "es-iife": "0.2.2",
    "html-minifier": "4.0.0",
    "istanbul-lib-coverage": "3.2.0",
    "istanbul-lib-instrument": "5.1.0",
    "istanbul-lib-report": "3.0.0",
    "istanbul-reports": "3.1.4",
    "pidtree": "0.5.0",
    "regenerator-runtime": "0.13.9",
    "strip-ansi": "7.0.1",
    "systemjs": "6.12.1",
    "terser": "5.12.1",
    "v8-to-istanbul": "8.1.1",
    "wrap-ansi": "8.0.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.17.0",
    "@babel/plugin-syntax-import-assertions": "7.16.7",
    "@jsenv/assert": "2.5.4",
    "@jsenv/eslint-config": "16.0.9",
    "@jsenv/file-size-impact": "12.1.11",
    "@jsenv/github-release-package": "1.3.6",
    "@jsenv/https-local": "1.0.8",
    "@jsenv/importmap-node-module": "5.1.3",
    "@jsenv/package-workspace": "0.0.5",
    "@jsenv/performance-impact": "2.2.10",
    "@jsenv/pwa": "5.0.0",
    "eslint": "8.13.0",
    "eslint-plugin-html": "6.2.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-react": "7.29.4",
    "playwright": "1.20.2",
    "postcss-import": "14.1.0",
    "preact": "10.7.1",
    "prettier": "2.6.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "redux": "4.1.2",
    "rollup": "2.70.1"
  }
}
