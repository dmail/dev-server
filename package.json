{
  "name": "@jsenv/core",
  "version": "37.1.2",
  "description": "Tool to develop, test and build js projects",
  "license": "MIT",
  "author": {
    "name": "dmail",
    "email": "dmaillard06@gmail.com",
    "url": "https://twitter.com/damienmaillard"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jsenv/core"
  },
  "engines": {
    "node": ">=18.5.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "imports": {},
  "exports": {
    ".": {
      "import": {
        "development": "./src/main.js",
        "default": "./dist/jsenv_core.js"
      }
    },
    "./*": "./*"
  },
  "main": "./src/main.js",
  "files": [
    "/dist/",
    "/src/"
  ],
  "workspaces": [
    "./packages/independent/*",
    "./packages/internal/*",
    "./packages/related/*",
    "./packages/related/create-jsenv/*"
  ],
  "scripts": {
    "eslint": "npx eslint . --ext=.js,.mjs,.cjs,.html",
    "dev": "node --conditions=development ./scripts/dev/dev.mjs",
    "test": "node --conditions=development ./scripts/test/test.mjs",
    "test:packages": "npm run test --workspaces --if-present -- --workspace",
    "snapshots:core": "npm run test -- --no-snapshot-assertion",
    "snapshots:packages": "npm run test --workspaces --if-present -- --workspace --no-snapshot-assertion",
    "snapshots:only_dev_server_errors": "node --conditions=development ./tests/dev_server/errors/generate_snapshot_files.mjs",
    "build": "node --conditions=development ./scripts/build/build.mjs",
    "build:file_size": "node ./scripts/build/build_file_size.mjs --log",
    "workspace:versions": "node ./scripts/publish/workspace_versions.mjs",
    "workspace:publish": "node ./scripts/publish/workspace_publish.mjs",
    "performances": "node --expose-gc ./scripts/performance/generate_performance_report.mjs --log --once",
    "prettier": "prettier --write .",
    "playwright:install": "npx playwright install-deps && npx playwright install",
    "certificate:install": "node ./scripts/dev/install_certificate_authority.mjs",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@financial-times/polyfill-useragent-normaliser": "1.10.2",
    "@jsenv/abort": "4.2.4",
    "@jsenv/ast": "5.0.0",
    "@jsenv/filesystem": "4.2.4",
    "@jsenv/importmap": "1.2.1",
    "@jsenv/integrity": "0.0.1",
    "@jsenv/log": "3.4.0",
    "@jsenv/node-esm-resolution": "1.0.1",
    "@jsenv/js-module-fallback": "1.2.0",
    "@jsenv/runtime-compat": "1.1.0",
    "@jsenv/server": "15.1.0",
    "@jsenv/sourcemap": "1.1.0",
    "@jsenv/plugin-transpilation": "1.2.1",
    "@jsenv/plugin-supervisor": "1.2.0",
    "@jsenv/url-meta": "8.1.0",
    "@jsenv/urls": "2.1.0",
    "@jsenv/utils": "2.0.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.22.7",
    "@babel/plugin-syntax-import-assertions": "7.22.5",
    "babel-plugin-transform-async-to-promises": "0.8.18",
    "@jsenv/assert": "./packages/independent/assert/",
    "@jsenv/core": "./",
    "@jsenv/eslint-config": "./packages/independent/eslint-config/",
    "@jsenv/file-size-impact": "14.1.1",
    "@jsenv/https-local": "3.0.7",
    "@jsenv/package-workspace": "0.5.2",
    "@jsenv/performance-impact": "4.1.1",
    "@jsenv/plugin-globals": "./packages/related/plugin-globals/",
    "@jsenv/plugin-minification": "./packages/related/plugin-minification/",
    "@jsenv/plugin-placeholders": "./packages/related/plugin-placeholders/",
    "@jsenv/plugin-as-js-classic": "./packages/related/plugin-as-js-classic/",
    "@jsenv/test": "./packages/related/test/",
    "eslint": "8.44.0",
    "eslint-plugin-html": "7.1.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-react": "7.32.2",
    "open": "9.1.0",
    "playwright": "1.35.1",
    "prettier": "3.0.0"
  }
}
