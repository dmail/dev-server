    "@jsenv/core": "34.2.0",
    "@jsenv/plugin-bundling": "2.1.5",
    "@jsenv/plugin-minification": "1.2.2",
    "@jsenv/eslint-config": "16.3.9",
    "@jsenv/eslint-import-resolver": "0.4.10",
    "eslint": "8.38.0",
    "eslint-plugin-html": "7.1.0",
    "eslint-plugin-import": "2.27.5",
    "open": "9.1.0",
    "playwright": "1.32.3",
    "prettier": "2.8.7"
