{
  "name": "@jsenv/eslint-import-resolver",
  "version": "0.1.6",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jsenv/jsenv-core",
    "directory": "packages/eslint-import-resolver"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./main.js",
      "require": "./dist/jsenv_eslint_import_resolver.cjs"
    },
    "./*": "./*"
  },
  "main": "./dist/jsenv_eslint_import_resolver.cjs",
  "files": [
    "/dist/",
    "/src/",
    "/main.js"
  ],
  "scripts": {
    "test": "node ./script/test.mjs",
    "build": "node  --experimental-import-meta-resolve ./script/build.mjs",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@jsenv/filesystem": "3.1.0",
    "@jsenv/importmap": "1.2.0",
    "@jsenv/node-esm-resolution": "0.0.5"
  },
  "devDependencies": {
    "rollup": "2.70.1"
  }
}
