{
  "name": "@jsenv/humanize",
  "version": "1.0.0",
  "description": "Generate messages meant to read by humans",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jsenv/core",
    "directory": "packages/independent/humanize"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14.7.0"
  },
  "type": "module",
  "exports": {
    ".": {
      "browser": {
        "development": "./src/main_browser.js",
        "default": "./dist/browser/jsenv_humanize.js"
