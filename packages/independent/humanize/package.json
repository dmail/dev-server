  "scripts": {
    "dev": "node ./scripts/dev.mjs",
    "build": "node ./scripts/build.mjs",
    "test": "node ./scripts/test.mjs"
  },
  "dependencies": {},
  "devDependencies": {
    "is-unicode-supported": "2.0.0",
    "supports-color": "9.4.0"
  }
}
