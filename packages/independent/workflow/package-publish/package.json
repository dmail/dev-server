{
  "name": "@jsenv/package-publish",
  "version": "1.11.0",
  "description": "Publish package to one or many registry.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jsenv/core",
    "directory": "packages/independent/workflow/package-publish"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./src/main.js"
    },
    "./*": "./*"
  },
  "main": "./src/main.js",
  "files": [
    "/src/"
  ],
  "scripts": {
    "test": "node --experimental-json-modules ./scripts/test.mjs"
  },
  "dependencies": {
    "@jsenv/filesystem": "4.10.2",
    "@jsenv/humanize": "1.2.8",
    "semver": "7.6.3"
  }
}
