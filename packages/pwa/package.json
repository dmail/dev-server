  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jsenv/jsenv-core",
    "directory": "packages/pwa"
  },
  "engines": {
    "node": ">=16.3.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
