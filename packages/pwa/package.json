    ".": {
      "import": "./src/main.js"
    },
    "./*": "./*"
  },
  "main": "./src/main.js",
  "files": [
    "/src/"
  ],
  "workspaces": [
    "./packages/*"
  ],
  "scripts": {
    "dev": "node ./script/dev/dev.mjs",
    "eslint": "node ./node_modules/eslint/bin/eslint.js . --ext=.js,.mjs,.jsx,.html --ignore-path=.eslintignore",
    "test": "node ./script/test/test.mjs",
    "test-with-coverage": "npm run test -- --coverage",
    "prettier": "prettier --write .",
    "packages-install": "cd docs/react_example && npm install",
    "certificate-install": "node ./script/dev/install_certificate.mjs"
  },
  "devDependencies": {}
