{
  "name": "jsenv-template-web-components",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "npm run dev -- --open",
    "dev": "node ./scripts/dev.mjs",
    "test": "node ./scripts/test.mjs",
    "test:coverage": "npm run test -- --coverage",
    "build": "node ./scripts/build.mjs",
    "build:serve": "node ./scripts/build_serve.mjs --open",
    "eslint": "npx eslint . --ext=.html,.js,.mjs,.cjs",
    "prettier": "prettier --write .",
    "playwright:install": "npx playwright install-deps && npx playwright install"
  },
  "devDependencies": {
    "@babel/plugin-syntax-import-assertions": "7.25.7",
    "@jsenv/custom-elements-redefine": "0.0.1",
    "@jsenv/assert": "4.4.1",
    "@jsenv/core": "39.5.15",
    "@jsenv/plugin-bundling": "2.7.13",
    "@jsenv/plugin-minification": "1.5.8",
    "@jsenv/eslint-config-relax": "1.2.7",
    "@jsenv/test": "3.5.19",
    "eslint": "9.11.1",
    "open": "10.1.0",
    "@playwright/browser-chromium": "1.47.2",
    "prettier": "3.3.3"
  }
}
