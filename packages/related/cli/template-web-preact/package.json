{
  "name": "jsenv-template-web-preact",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "npm run dev -- --open",
    "dev": "node ./scripts/dev.mjs",
    "test": "node ./scripts/test.mjs",
    "test:coverage": "npm run test -- --coverage",
    "build": "node ./scripts/build.mjs",
    "build:serve": "node ./scripts/build_serve.mjs --open",
    "eslint": "npx eslint . --ext=.html,.js,.jsx,.mjs,.cjs",
    "prettier": "prettier --write .",
    "playwright:install": "npx playwright install-deps && npx playwright install"
  },
  "dependencies": {
    "preact": "10.23.2"
  },
  "devDependencies": {
    "@babel/plugin-syntax-import-assertions": "7.24.7",
    "@babel/plugin-transform-react-jsx": "7.25.2",
    "@jsenv/assert": "4.4.0",
    "@jsenv/core": "39.3.12",
    "@jsenv/plugin-preact": "1.6.16",
    "@jsenv/plugin-bundling": "2.7.7",
    "@jsenv/plugin-minification": "1.5.5",
    "@jsenv/eslint-config-relax": "1.1.1",
    "@jsenv/test": "3.4.1",
    "eslint": "9.9.0",
    "open": "10.1.0",
    "@playwright/browser-chromium": "1.46.0",
    "prettier": "3.3.3"
  }
}
