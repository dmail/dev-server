{
  "name": "jsenv-template-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "npm run dev -- --open",
    "dev": "node ./scripts/dev.mjs",
    "test": "node ./scripts/test.mjs",
    "test:coverage": "npm run test -- --coverage",
    "build": "node ./scripts/build.mjs",
    "build:serve": "node ./scripts/build_serve.mjs --open",
    "eslint": "npx eslint . --ext=.html,.js,.mjs,.cjs",
    "prettier": "prettier --write ."
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.25.1",
    "@babel/plugin-syntax-import-assertions": "7.24.7",
    "@jsenv/assert": "4.4.0",
    "@jsenv/core": "39.3.12",
    "@jsenv/eslint-config-relax": "1.0.0",
    "@jsenv/plugin-bundling": "2.7.7",
    "@jsenv/plugin-minification": "1.5.5",
    "@jsenv/test": "3.4.1",
    "eslint": "9.9.0",
    "open": "10.1.0",
    "@playwright/browser-chromium": "1.46.0",
    "prettier": "3.3.3"
  }
}
