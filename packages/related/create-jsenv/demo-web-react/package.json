    "@jsenv/plugin-minification": "1.5.0",
    "@jsenv/eslint-config": "16.4.2",
    "@jsenv/eslint-import-resolver": "8.0.2",
    "@jsenv/test": "1.5.2",
    "eslint": "8.45.0",
    "eslint-plugin-html": "7.1.0",
