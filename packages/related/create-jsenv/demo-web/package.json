    "@jsenv/eslint-config": "16.4.0",
    "@jsenv/eslint-import-resolver": "8.0.0",
    "@jsenv/test": "1.4.0",
    "eslint": "8.44.0",
    "eslint-plugin-html": "7.1.0",
    "eslint-plugin-import": "2.27.5",
    "open": "9.1.0",
