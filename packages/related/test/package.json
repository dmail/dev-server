{
  "name": "@jsenv/test",
  "version": "1.3.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jsenv/core",
    "directory": "packages/related/test"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "development": "./src/main.js",
        "default": "./dist/jsenv_test.js"
      }
    },
    "./*": "./*"
  },
  "files": [
    "/dist/",
    "/src/"
  ],
  "scripts": {
    "test": "node --conditions=development ./scripts/test.mjs",
    "build": "node --conditions=development ./scripts/build.mjs",
    "prepublishOnly": "npm run build"
  },
  "optionalDependencies": {
    "playwright": "1.x"
  },
  "dependencies": {
    "@jsenv/abort": "4.2.4",
    "@jsenv/ast": "4.1.0",
    "@jsenv/filesystem": "4.2.3",
    "@jsenv/log": "3.4.0",
    "@jsenv/plugin-supervisor": "1.1.1",
    "@jsenv/urls": "2.0.0",
    "@jsenv/url-meta": "8.1.0",
    "@jsenv/uneval": "1.6.0",
    "@jsenv/utils": "2.0.1",
    "v8-to-istanbul": "9.1.0",
    "istanbul-lib-coverage": "3.2.0",
    "istanbul-lib-instrument": "5.2.1",
    "istanbul-lib-report": "3.0.0",
    "istanbul-reports": "3.1.5",
    "@paralleldrive/cuid2": "2.2.1",
    "@c88/v8-coverage": "0.1.1",
    "strip-ansi": "7.1.0",
    "wrap-ansi": "8.1.0",
    "pidtree": "0.6.0"
  }
}
