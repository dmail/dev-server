{
  "name": "@jsenv/test",
  "version": "3.0.25",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jsenv/core",
    "directory": "packages/related/test"
  },
  "engines": {
    "node": ">=20.11.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "development": "./src/main.js",
        "default": "./dist/jsenv_test.js"
      }
    },
    "./*": "./*"
  },
  "files": [
    "/dist/",
    "/src/"
  ],
  "scripts": {
    "test": "node --conditions=development ./scripts/test.mjs",
    "build": "node --conditions=development ./scripts/build.mjs",
    "prepublishOnly": "npm run build"
  },
  "optionalDependencies": {
    "playwright": "1.x"
  },
  "dependencies": {
    "@c88/v8-coverage": "0.1.1",
    "@jsenv/abort": "4.3.0",
    "@jsenv/ast": "6.0.7",
    "@jsenv/filesystem": "4.7.0",
    "@jsenv/github-check-run": "0.1.14",
    "@jsenv/humanize": "1.1.3",
    "@jsenv/plugin-supervisor": "1.4.10",
    "@jsenv/url-meta": "8.4.0",
    "@jsenv/urls": "2.2.7",
    "@jsenv/utils": "2.1.1",
    "istanbul-lib-coverage": "3.2.2",
    "istanbul-lib-instrument": "6.0.3",
    "istanbul-lib-report": "3.0.1",
    "istanbul-reports": "3.1.7",
    "he": "1.2.0",
    "pidtree": "0.6.0",
    "strip-ansi": "7.1.0",
    "supports-color": "9.4.0",
    "v8-to-istanbul": "9.3.0",
    "errorstacks": "2.4.1"
  },
  "devDependencies": {
    "@jsenv/terminal-recorder": "../independent/terminal-recorder/"
  }
}
